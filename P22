
void Controlador::Display(AC *areaC) {
//Despliega informacion en pantalla
          for (int i = 0; i < areaC->n; i++) {
                    if(areaC->Etiquetas[i].veces >= 0) {
                              printf("Palabra: %s ", areaC->Etiquetas[i].palabra);
                              cout << "Total: " << areaC->Etiquetas[i].veces << endl;
                    }
          }
};





